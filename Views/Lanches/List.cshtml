@model LancheListViewModel
@{
    ViewData["Title"] = "Todos os Lanches";
}
<div>
    <h2>@Model.CategoriaAtual</h2>
</div>
<hr />
<br />

<div class="container">
    <div class="row">

        @foreach (var item in Model.Lanches)
        {
            <partial name="_LanchesResumo" for="@item" />
        }

    </div>
</div>